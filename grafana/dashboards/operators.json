{
  "uid": "callcenter-operators",
  "title": "Call Center AI â€” Operators",
  "description": "Operator monitoring dashboard: queue length, transfer reasons, online/offline status",
  "tags": ["call-center", "operators"],
  "editable": true,
  "graphTooltip": 1,
  "refresh": "30s",
  "schemaVersion": 39,
  "time": {
    "from": "now-24h",
    "to": "now"
  },
  "timepicker": {
    "refresh_intervals": ["5s", "10s", "30s", "1m", "5m"],
    "time_options": ["5m", "15m", "1h", "6h", "12h", "24h", "2d", "7d"]
  },
  "timezone": "browser",
  "panels": [
    {
      "id": 1,
      "title": "Operator Queue Length",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "operator_queue_length",
          "legendFormat": "Queue Length",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 20,
            "pointSize": 5,
            "drawStyle": "line",
            "showPoints": "auto"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 3 },
              { "color": "red", "value": 5 }
            ]
          }
        }
      }
    },
    {
      "id": 2,
      "title": "Transfers to Operator (rate/5m)",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "rate(transfers_to_operator_total[5m])",
          "legendFormat": "Transfers/5m",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 10,
            "drawStyle": "line"
          }
        }
      }
    },
    {
      "id": 3,
      "title": "Transfer Reasons",
      "type": "piechart",
      "gridPos": { "h": 8, "w": 8, "x": 0, "y": 8 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "transfers_to_operator_total",
          "legendFormat": "{{reason}}",
          "refId": "A"
        }
      ],
      "options": {
        "legend": { "displayMode": "table", "placement": "right" },
        "pieType": "donut",
        "reduceOptions": { "calcs": ["lastNotNull"] }
      }
    },
    {
      "id": 4,
      "title": "Average Wait Time (seconds)",
      "type": "stat",
      "gridPos": { "h": 4, "w": 8, "x": 8, "y": 8 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "avg(operator_wait_time_seconds)",
          "legendFormat": "Avg Wait",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 30 },
              { "color": "red", "value": 60 }
            ]
          }
        }
      },
      "options": {
        "colorMode": "background",
        "graphMode": "none",
        "textMode": "value"
      }
    },
    {
      "id": 5,
      "title": "Operators Online",
      "type": "stat",
      "gridPos": { "h": 4, "w": 8, "x": 16, "y": 8 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "count(operator_status == 1)",
          "legendFormat": "Online",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "yellow", "value": 1 },
              { "color": "green", "value": 2 }
            ]
          }
        }
      },
      "options": {
        "colorMode": "background",
        "graphMode": "none",
        "textMode": "value"
      }
    },
    {
      "id": 6,
      "title": "Operator Status Table",
      "type": "table",
      "gridPos": { "h": 8, "w": 16, "x": 8, "y": 12 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "operator_status",
          "legendFormat": "{{operator_name}}",
          "refId": "A",
          "format": "table",
          "instant": true
        }
      ],
      "fieldConfig": {
        "defaults": {},
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "Value" },
            "properties": [
              {
                "id": "mappings",
                "value": [
                  { "type": "value", "options": { "0": { "text": "Offline", "color": "red" } } },
                  { "type": "value", "options": { "1": { "text": "Online", "color": "green" } } },
                  { "type": "value", "options": { "2": { "text": "Busy", "color": "yellow" } } },
                  { "type": "value", "options": { "3": { "text": "Break", "color": "blue" } } }
                ]
              }
            ]
          }
        ]
      },
      "options": {
        "showHeader": true,
        "sortBy": [{ "displayName": "operator_name", "desc": false }]
      }
    }
  ]
}
