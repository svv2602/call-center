    {
      "id": 6,
      "title": "PostgreSQL Query Duration (p95)",
      "description": "95th percentile query duration from pg_stat_statements",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 10 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "refId": "A",
          "datasource": { "type": "prometheus", "uid": "prometheus" },
