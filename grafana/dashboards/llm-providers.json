{
  "uid": "llm-providers",
  "title": "LLM Multi-Provider Metrics",
  "tags": ["call-center", "llm"],
  "refresh": "30s",
  "schemaVersion": 39,
  "time": { "from": "now-3h", "to": "now" },
  "timepicker": {},
  "timezone": "browser",
  "fiscalYearStartMonth": 0,
  "graphTooltip": 0,
  "links": [],
  "panels": [
    {
      "id": 100,
      "title": "LLM Overview",
      "type": "row",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 0 },
      "collapsed": false,
      "panels": []
    },
    {
      "id": 1,
      "title": "Request Rate",
      "type": "stat",
      "gridPos": { "h": 6, "w": 6, "x": 0, "y": 1 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "refId": "A",
          "datasource": { "type": "prometheus", "uid": "prometheus" },
          "expr": "sum(rate(callcenter_llm_requests_total{provider=~\"$provider\"}[$__rate_interval]))",
          "legendFormat": "",
          "editorMode": "code",
          "range": true,
          "instant": false
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null }
            ]
          },
          "unit": "reqps"
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": { "values": false, "calcs": ["lastNotNull"], "fields": "" },
        "colorMode": "background_solid",
        "graphMode": "area",
        "textMode": "auto"
      }
    },
    {
      "id": 2,
      "title": "Error Rate",
      "type": "stat",
      "gridPos": { "h": 6, "w": 6, "x": 6, "y": 1 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "refId": "A",
          "datasource": { "type": "prometheus", "uid": "prometheus" },
          "expr": "sum(rate(callcenter_llm_errors_total{provider=~\"$provider\"}[$__rate_interval]))",
          "legendFormat": "",
          "editorMode": "code",
          "range": true,
          "instant": false
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "red", "value": 0.1 }
            ]
          },
          "unit": "reqps"
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": { "values": false, "calcs": ["lastNotNull"], "fields": "" },
        "colorMode": "background_solid",
        "graphMode": "area",
        "textMode": "auto"
      }
    },
    {
      "id": 3,
      "title": "Fallbacks",
      "type": "stat",
      "gridPos": { "h": 6, "w": 6, "x": 12, "y": 1 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "refId": "A",
          "datasource": { "type": "prometheus", "uid": "prometheus" },
          "expr": "sum(rate(callcenter_llm_fallbacks_total[$__rate_interval]))",
          "legendFormat": "",
          "editorMode": "code",
          "range": true,
          "instant": false
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 0.01 }
            ]
          },
          "unit": "reqps"
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": { "values": false, "calcs": ["lastNotNull"], "fields": "" },
        "colorMode": "background_solid",
        "graphMode": "area",
        "textMode": "auto"
      }
    },
    {
      "id": 4,
      "title": "p95 Latency",
      "type": "stat",
      "gridPos": { "h": 6, "w": 6, "x": 18, "y": 1 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "refId": "A",
          "datasource": { "type": "prometheus", "uid": "prometheus" },
          "expr": "histogram_quantile(0.95, sum by (le) (rate(callcenter_llm_provider_latency_ms_bucket{provider=~\"$provider\"}[$__rate_interval])))",
          "legendFormat": "",
          "editorMode": "code",
          "range": true,
          "instant": false
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 1000 },
              { "color": "red", "value": 3000 }
            ]
          },
          "unit": "ms"
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": { "values": false, "calcs": ["lastNotNull"], "fields": "" },
        "colorMode": "background_solid",
        "graphMode": "area",
        "textMode": "auto"
      }
    },
    {
      "id": 101,
      "title": "Provider Breakdown",
      "type": "row",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 7 },
      "collapsed": false,
      "panels": []
    },
    {
      "id": 5,
      "title": "Latency by Provider (p50/p95)",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 8 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "refId": "A",
          "datasource": { "type": "prometheus", "uid": "prometheus" },
          "expr": "histogram_quantile(0.50, sum by (le, provider) (rate(callcenter_llm_provider_latency_ms_bucket{provider=~\"$provider\"}[$__rate_interval])))",
          "legendFormat": "p50 {{provider}}",
          "editorMode": "code",
          "range": true,
          "instant": false
        },
        {
          "refId": "B",
          "datasource": { "type": "prometheus", "uid": "prometheus" },
          "expr": "histogram_quantile(0.95, sum by (le, provider) (rate(callcenter_llm_provider_latency_ms_bucket{provider=~\"$provider\"}[$__rate_interval])))",
          "legendFormat": "p95 {{provider}}",
          "editorMode": "code",
          "range": true,
          "instant": false
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "color": { "mode": "palette-classic" },
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "lineWidth": 2,
            "fillOpacity": 10,
            "showPoints": "never"
          }
        },
        "overrides": []
      },
      "options": {
        "tooltip": { "mode": "multi", "sort": "desc" },
        "legend": { "displayMode": "list", "placement": "bottom", "showLegend": true }
      }
    },
    {
      "id": 6,
      "title": "Requests by Provider & Task",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 8 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "refId": "A",
          "datasource": { "type": "prometheus", "uid": "prometheus" },
          "expr": "sum by (provider, task) (rate(callcenter_llm_requests_total{provider=~\"$provider\"}[$__rate_interval]))",
          "legendFormat": "{{provider}} / {{task}}",
          "editorMode": "code",
          "range": true,
          "instant": false
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "color": { "mode": "palette-classic" },
          "custom": {
            "drawStyle": "bars",
            "lineInterpolation": "smooth",
            "lineWidth": 2,
            "fillOpacity": 50,
            "showPoints": "never"
          }
        },
        "overrides": []
      },
      "options": {
        "tooltip": { "mode": "multi", "sort": "desc" },
        "legend": { "displayMode": "list", "placement": "bottom", "showLegend": true }
      }
    },
    {
      "id": 102,
      "title": "Fallbacks & Errors",
      "type": "row",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 16 },
      "collapsed": false,
      "panels": []
    },
    {
      "id": 7,
      "title": "Fallback Events",
      "type": "table",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 17 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "refId": "A",
          "datasource": { "type": "prometheus", "uid": "prometheus" },
          "expr": "sum by (from_provider, to_provider, task) (increase(callcenter_llm_fallbacks_total[$__range]))",
          "legendFormat": "",
          "editorMode": "code",
          "range": false,
          "instant": true,
          "format": "table"
        }
      ],
      "fieldConfig": { "defaults": {}, "overrides": [] },
      "options": { "showHeader": true, "footer": { "show": false } },
      "transformations": [{ "id": "organize", "options": {} }]
    },
    {
      "id": 8,
      "title": "Error Rate by Provider",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 17 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "refId": "A",
          "datasource": { "type": "prometheus", "uid": "prometheus" },
          "expr": "sum by (provider) (rate(callcenter_llm_errors_total{provider=~\"$provider\"}[$__rate_interval]))",
          "legendFormat": "{{provider}}",
          "editorMode": "code",
          "range": true,
          "instant": false
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "color": { "mode": "palette-classic" },
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "lineWidth": 2,
            "fillOpacity": 10,
            "showPoints": "never"
          }
        },
        "overrides": []
      },
      "options": {
        "tooltip": { "mode": "multi", "sort": "desc" },
        "legend": { "displayMode": "list", "placement": "bottom", "showLegend": true }
      }
    }
  ],
  "templating": {
    "list": [
      {
        "name": "provider",
        "type": "query",
        "label": "Provider",
        "datasource": { "type": "prometheus", "uid": "prometheus" },
        "definition": "label_values(callcenter_llm_requests_total, provider)",
        "query": {
          "query": "label_values(callcenter_llm_requests_total, provider)",
          "refId": "StandardVariableQuery"
        },
        "refresh": 2,
        "includeAll": true,
        "allValue": ".*",
        "multi": true,
        "sort": 1,
        "hide": 0,
        "current": {}
      }
    ]
  },
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": { "type": "grafana", "uid": "-- Grafana --" },
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  }
}
