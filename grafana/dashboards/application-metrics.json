      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "refId": "A",
          "datasource": { "type": "prometheus", "uid": "prometheus" },
          "expr": "histogram_quantile(0.5, rate(callcenter_call_duration_seconds_bucket[$__rate_interval]))",
          "legendFormat": "p50",
          "editorMode": "code",
          "range": true,
          "instant": false
