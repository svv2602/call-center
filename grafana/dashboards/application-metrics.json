    {
      "id": 1,
      "title": "Active Calls",
      "description": "Current number of active calls being processed by the AI agent",
      "type": "stat",
      "gridPos": { "h": 8, "w": 6, "x": 0, "y": 1 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "refId": "A",
          "datasource": { "type": "prometheus", "uid": "prometheus" },
          "expr": "callcenter_active_calls",
          "legendFormat": "Active Calls",
          "editorMode": "code",
          "range": true,
          "instant": false
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 20 },
              { "color": "red", "value": 40 }
            ]
          },
          "mappings": [],
          "unit": "none"
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": {
          "values": false,
          "calcs": ["lastNotNull"],
          "fields": ""
        },
