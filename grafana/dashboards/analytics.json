        {
          "refId": "A",
          "datasource": {
            "type": "grafana-postgresql-datasource",
            "uid": "postgresql"
          },
          "rawSql": "SELECT stat_date, total_calls, resolved_by_bot, transferred, avg_duration_seconds, avg_quality_score, total_cost_usd FROM daily_stats ORDER BY stat_date DESC LIMIT 30",
