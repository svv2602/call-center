    {
      "id": 8,
      "title": "Daily Stats (PostgreSQL)",
      "description": "Aggregated daily statistics from the daily_stats table in PostgreSQL",
      "type": "table",
      "gridPos": {
        "h": 10,
        "w": 24,
        "x": 0,
        "y": 23
