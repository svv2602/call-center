{
  "uid": "callcenter-analytics",
  "title": "Call Center AI â€” Analytics",
  "description": "Aggregated analytics dashboard for Call Center AI system",
  "tags": ["call-center", "analytics"],
  "editable": true,
  "graphTooltip": 1,
  "refresh": "5m",
  "schemaVersion": 39,
  "fiscalYearStartMonth": 0,
  "liveNow": false,
  "time": {
    "from": "now-7d",
    "to": "now"
  },
  "timepicker": {
    "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h"],
    "time_options": ["5m", "15m", "1h", "6h", "12h", "24h", "2d", "7d", "30d"]
  },
  "timezone": "browser",
  "templating": {
    "list": [
      {
        "name": "time_grouping",
        "label": "Time Grouping",
        "type": "custom",
        "current": {
          "selected": true,
          "text": "1d",
          "value": "1d"
        },
        "options": [
          {
            "selected": true,
            "text": "1d",
            "value": "1d"
          },
          {
            "selected": false,
            "text": "1w",
            "value": "1w"
          },
          {
            "selected": false,
            "text": "30d",
            "value": "30d"
          }
        ],
        "query": "1d,1w,30d",
        "hide": 0,
        "includeAll": false,
        "multi": false,
        "skipUrlSync": false
      }
    ]
  },
  "panels": [
    {
      "id": 1,
      "title": "Calls per Day/Week/Month",
      "description": "Total number of calls aggregated by the selected time grouping interval",
      "type": "barchart",
      "gridPos": {
        "h": 9,
        "w": 12,
        "x": 0,
        "y": 0
      },
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "refId": "A",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          },
          "expr": "increase(callcenter_calls_total[$time_grouping])",
          "legendFormat": "Calls",
          "interval": "$time_grouping",
          "editorMode": "code",
          "range": true,
          "instant": false
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "axisBorderShow": false,
            "axisCenteredZero": false,
            "axisColorMode": "text",
            "axisLabel": "Number of Calls",
            "axisPlacement": "auto",
            "fillOpacity": 80,
            "gradientMode": "none",
            "hideFrom": {
              "legend": false,
              "tooltip": false,
              "viz": false
            },
            "lineWidth": 1,
            "scaleDistribution": {
              "type": "linear"
            },
            "thresholdsStyle": {
              "mode": "off"
            }
          },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              }
            ]
          },
          "unit": "short"
        },
        "overrides": []
      },
      "options": {
        "barRadius": 0.1,
        "barWidth": 0.8,
        "fullHighlight": false,
        "groupWidth": 0.7,
        "legend": {
          "calcs": ["sum"],
          "displayMode": "list",
          "placement": "bottom",
          "showLegend": true
        },
        "orientation": "auto",
        "showValue": "auto",
        "stacking": "none",
        "tooltip": {
          "mode": "single",
          "sort": "none"
        },
        "xTickLabelRotation": 0
      }
    },
    {
      "id": 2,
      "title": "Bot vs Operator",
      "description": "Distribution of calls resolved by the AI bot versus transferred to a human operator",
      "type": "piechart",
      "gridPos": {
        "h": 9,
        "w": 6,
        "x": 12,
        "y": 0
      },
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "refId": "A",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          },
          "expr": "callcenter_calls_resolved_by_bot_total",
          "legendFormat": "Resolved by Bot",
          "instant": true,
          "range": false,
          "editorMode": "code"
        },
        {
          "refId": "B",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          },
          "expr": "callcenter_transfers_to_operator_total",
          "legendFormat": "Transferred to Operator",
          "instant": true,
          "range": false,
          "editorMode": "code"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "hideFrom": {
              "legend": false,
              "tooltip": false,
              "viz": false
            }
          },
          "mappings": [],
          "unit": "short"
        },
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "Resolved by Bot"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "fixedColor": "green",
                  "mode": "fixed"
                }
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "Transferred to Operator"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "fixedColor": "orange",
                  "mode": "fixed"
                }
              }
            ]
          }
        ]
      },
      "options": {
        "legend": {
          "calcs": ["percent"],
          "displayMode": "list",
          "placement": "bottom",
          "showLegend": true,
          "values": ["percent"]
        },
        "pieType": "donut",
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "fields": "",
          "values": false
        },
        "tooltip": {
          "mode": "single",
          "sort": "none"
        },
        "displayLabels": ["name", "percent"]
      }
    },
    {
      "id": 3,
      "title": "Scenario Distribution",
      "description": "Breakdown of calls by scenario type (tire search, order, fitting, etc.)",
      "type": "piechart",
      "gridPos": {
        "h": 9,
        "w": 6,
        "x": 18,
        "y": 0
      },
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "refId": "A",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          },
          "expr": "callcenter_call_scenario_total",
          "legendFormat": "{{scenario}}",
          "instant": true,
          "range": false,
          "editorMode": "code"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "hideFrom": {
              "legend": false,
              "tooltip": false,
              "viz": false
            }
          },
          "mappings": [],
          "unit": "short"
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "calcs": ["percent"],
          "displayMode": "list",
          "placement": "right",
          "showLegend": true,
          "values": ["percent"]
        },
        "pieType": "donut",
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "fields": "",
          "values": false
        },
        "tooltip": {
          "mode": "single",
          "sort": "none"
        },
        "displayLabels": ["name", "percent"]
      }
    },
    {
      "id": 4,
      "title": "Average Call Cost (Median, USD)",
      "description": "Median (p50) call cost over time, computed from the callcenter_call_cost_usd histogram",
      "type": "timeseries",
      "gridPos": {
        "h": 9,
        "w": 12,
        "x": 0,
        "y": 9
      },
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "refId": "A",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          },
          "expr": "histogram_quantile(0.5, rate(callcenter_call_cost_usd_bucket[5m]))",
          "legendFormat": "Median Cost (p50)",
          "editorMode": "code",
          "range": true,
          "instant": false
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "axisBorderShow": false,
            "axisCenteredZero": false,
            "axisColorMode": "text",
            "axisLabel": "Cost (USD)",
            "axisPlacement": "auto",
            "barAlignment": 0,
            "drawStyle": "line",
            "fillOpacity": 15,
            "gradientMode": "scheme",
            "hideFrom": {
              "legend": false,
              "tooltip": false,
              "viz": false
            },
            "insertNulls": false,
            "lineInterpolation": "smooth",
            "lineWidth": 2,
            "pointSize": 5,
            "scaleDistribution": {
              "type": "linear"
            },
            "showPoints": "auto",
            "spanNulls": false,
            "stacking": {
              "group": "A",
              "mode": "none"
            },
            "thresholdsStyle": {
              "mode": "off"
            }
          },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "yellow",
                "value": 0.5
              },
              {
                "color": "red",
                "value": 1.0
              }
            ]
          },
          "unit": "currencyUSD"
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "calcs": ["mean", "max", "lastNotNull"],
          "displayMode": "list",
          "placement": "bottom",
          "showLegend": true
        },
        "tooltip": {
          "mode": "single",
          "sort": "none"
        }
      }
    },
    {
      "id": 5,
      "title": "Calls by Hour of Day",
      "description": "Heatmap showing call volume distribution across hours of the day",
      "type": "heatmap",
      "gridPos": {
        "h": 9,
        "w": 12,
        "x": 12,
        "y": 9
      },
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "refId": "A",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          },
          "expr": "rate(callcenter_calls_total[5m])",
          "legendFormat": "{{instance}}",
          "editorMode": "code",
          "range": true,
          "instant": false,
          "format": "heatmap"
        }
